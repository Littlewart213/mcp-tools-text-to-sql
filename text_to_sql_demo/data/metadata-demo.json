      },
      {
        "column_name": "operator",
        "data_type": "TEXT",
        "description": "Company operating the drilling program.",
        "example": "Nusantara Energy"
      },
      {
        "column_name": "basin",
        "data_type": "TEXT",
        "description": "Sedimentary basin for the well.",
